#include <iostream>
using namespace std;

int main() {
    int n, key;
    int a[50];

    cout << "Enter number of elements: ";
    cin >> n;

    cout << "Enter elements:\n";
    for(int i = 0; i < n; i++)
        cin >> a[i];

    cout << "Enter element to search: ";
    cin >> key;

    int flag = 0;

    for(int i = 0; i < n; i++) {
        if(a[i] == key) {
            cout << "Element found at position " << i + 1;
            flag = 1;
            break;
        }
    }

    if(flag == 0)
        cout << "Element not found";

    return 0;
}
